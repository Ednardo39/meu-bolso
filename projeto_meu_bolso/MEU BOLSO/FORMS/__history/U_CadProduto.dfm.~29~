  inherited Q_Padrao: TFDQuery
    SQL.Strings = (
      'SELECT A.ID_PRODUTO,'
      '       A.PRODUTO_DESCRICAO,'
      '       A.VL_CUSTO,'
      '       A.VL_VENDA,'
      '       A.ESTOQUE,'
      '       A.ESTOQUE_MIN,'
      '       A.UNIDADE,'
      '       A.CADASTRO,'
      '       A.ID_FORNECEDOR,'
      '       B.NOME'
      ' FROM PRODUTO A, FORNECEDOR B'
      ' WHERE A.ID_FORNECEDOR=B.ID_FORNECEDOR')
    Left = 720
    Top = 89
